#include <stdio.h>

// Function to compare two strings
int compareStrings(char str1[], char str2[]) {
    // write your code here
int i = 0;
	while(str1[i] !='\0' && str2[i] != '\0'){
		if(str1[i] != str2[i]) {
			return -1;
		}
		i++;
	}
	if (str1[i] == '\0' && str2[i] == '\0')
		return 0;
	else 
		return -1;
}

// Function to find the longer string
void findLongerString(char str1[], char str2[]) {
int len1 = 0, len2 = 0;
	while (str1[len1] != '\0') len1++;
	while (str2[len2] != '\0') len2++;
	if (len1 >= len2)
		printf("Longer string: %s\n", str1);
	else
		printf("Longer string: %s\n", str2);
}

// Function to concatenate two strings
void concatenateStrings(char str1[], char str2[], char result[]) {
int i = 0, j = 0 ;
	while (str1[i] != '\0') {
	result[i] = str1[i];
	i++;
}
	while (str2[j] != '\0') {
	result[i] = str2[j];
	i++;
	j++;
}
	result[i] = '\0';
}

// Function to find occurrence of substring in the main string
int findSubstring(char mainStr[], char subStr[]) {
int i,j;

	for (i = 0; mainStr[i] != '\0'; i++){
		j = 0;
	while(mainStr[i + j] != '\0' && subStr[j] != '\0' && mainStr[i+j] == subStr[j]) {
			j++;
		}
		if (subStr[j] == '\0'){
			return i;
		}
	}
	return -1;
}

