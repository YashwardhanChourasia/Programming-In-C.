#include<stdio.h>
void main(){
	int m,n;
	printf("Enter no of rows, columns: ");
	scanf("%d %d",&m,&n);
	int mat[m] [n];
	printf("Enter elements of matrix:\n");
	for(int i=0;i<m;i++){
		for(int j=0;j<n;j++){
			scanf("%d",&mat[i][j]);
		}
	}
	if(m != n){
		printf("Matrix must be square matrix\n");
		return; 
	}
	int isLTM=1;
	for(int i=0; i<m; i++){
		for(int j=0; j<m; j++){
			if(mat[i][j]!=0 && (j>i)){
				isLTM =0;
			}
		}
	}
	if(isLTM == 1){
		printf("Lower triangular matrix\n");
	}
	else{
		printf("Not a lower triangular matrix\n");
	}
}

