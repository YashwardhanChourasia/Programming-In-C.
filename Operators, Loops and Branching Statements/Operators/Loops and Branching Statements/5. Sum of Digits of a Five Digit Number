#include <stdio.h>
int main () {
	int num, sum = 0;

	scanf("%d" , &num);
	if (num >= 10000 && num <= 99999) {
	sum += num % 10;
	num /=10;

	sum += num % 10;
	num /=10;

	sum += num % 10;
	num /=10;

	sum += num % 10;
	num /=10;

	sum += num % 10;
	printf("%d\n",sum);
	} else {
		printf("Invalid input\n");
	}
	return 0;
}
